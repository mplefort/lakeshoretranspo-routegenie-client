{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*"]
    },
    "strict": true,
    "rootDir": "src",
    "jsx": "react-jsx",
    "lib": ["ES6", "DOM", "DOM.Iterable"]
  },
  "include": ["src/**/*"]
}
